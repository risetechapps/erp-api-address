{
    "name": "risetechapps/address",
    "description": "",
    "keywords": [
        "risetechapps",
        "address"
    ],
    "homepage": "https://github.com/risetechapps/address",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Rise Tech",
            "email": "apps@risetech.com.br",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0",
        "risetechapps/form-request": "^3.0.1",
        "risetechapps/repository": "^2.0.8",
        "risetechapps/to-upper": "^1.1"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "RiseTech\\Address\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RiseTech\\Address\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "RiseTech\\Address\\AddressServiceProvider"
            ],
            "aliases": {
                "Address": "RiseTech\\Address\\AddressFacade"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
